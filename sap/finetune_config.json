{
  "train_dir": "/path/to/patch_train",
  "val_dir": "/path/to/patch_val",
  "output_dir": "./runs",
  "run_name": "sap_finetune",
  "training_mode": "SAP",
  "sap_checkpoint": "/path/to/sap_checkpoint.pt",
  "ssl_checkpoint": null,
  "max_epochs": 400,
  "batch_size": 2,
  "val_every": 5,
  "roi_x": 96,
